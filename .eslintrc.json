{
  "extends": ["eslint:recommended", "airbnb-base"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "no-console": "off",
    "no-unused-vars": "warn"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
